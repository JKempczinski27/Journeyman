{
  "name": "journeyman-security-tests",
  "version": "1.0.0",
  "description": "Comprehensive security testing suite for Journeyman NFL game",
  "main": "test-runner.js",
  "scripts": {
    "test": "node test-runner.js all",
    "test:quick": "node test-runner.js quick http://localhost:3001",
    "test:local": "node test-runner.js single http://localhost:3001",
    "test:prod": "node test-runner.js single https://journeyman-production.up.railway.app",
    "install-deps": "npm install axios",
    "setup": "npm run install-deps && mkdir -p reports",
    "clean": "rm -rf reports/*.json"
  },
  "dependencies": {
    "axios": "^1.12.0",
    "bcrypt": "^6.0.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "rate-limiter-flexible": "^7.3.0"
  },
  "keywords": [
    "security",
    "penetration-testing",
    "owasp",
    "vulnerability-assessment"
  ],
  "author": "Journeyman Security Team",
  "license": "MIT"
}
